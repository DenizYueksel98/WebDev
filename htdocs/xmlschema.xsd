<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<xs:element name="db">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="car" type="cartype"
					maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="cartype">
		<xs:sequence>
			<xs:element name="id" type="int999" maxOccurs="1" />
			<xs:element name="name" type="xs:string" maxOccurs="1" />
			<xs:element name="schein" type="scheintyp" maxOccurs="1" />
			<xs:element name="nefz" type="nefztyp" maxOccurs="1" />
			<xs:element name="wltp" type="wltptyp" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="scheintyp">
		<xs:sequence>
			<xs:element name="b21" type="int4" maxOccurs="1" />
			<xs:element name="b22" type="int3" maxOccurs="1" />
			<xs:element name="j" type="string2" maxOccurs="1" />
			<xs:element name="vier" type="int4" maxOccurs="1" />
			<xs:element name="d1" type="string30" maxOccurs="1" />
			<xs:element name="d21" type="string8" maxOccurs="1" />
			<xs:element name="d22" type="string8" maxOccurs="1" />
			<xs:element name="d23" type="string8" maxOccurs="1" />
			<xs:element name="zwei" type="string30" maxOccurs="1" />
			<xs:element name="fuenf1" type="string30" maxOccurs="1" />
			<xs:element name="fuenf2" type="string30" maxOccurs="1" />
			<xs:element name="v9" type="string30" maxOccurs="1" />
			<xs:element name="vierzehn" type="string30" maxOccurs="1" />
			<xs:element name="p3" type="fuel" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="nefztyp">
		<xs:sequence>
			<xs:element name="verbin" type="consumption" maxOccurs="1" />
			<xs:element name="verbau" type="consumption" maxOccurs="1" />
			<xs:element name="verbko" type="consumption" maxOccurs="1" />
			<xs:element name="co2kom" type="co2" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="wltptyp">
		<xs:sequence>
			<xs:element name="sehrs" type="wltptyp_km" maxOccurs="1" />
			<xs:element name="schnell" type="wltptyp_km" maxOccurs="1" />
			<xs:element name="langsam" type="wltptyp_km" maxOccurs="1" />
			<xs:element name="co2kom" type="co2" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<xs:simpleType name="int4">
		<xs:restriction base="xs:int">
			<xs:pattern value="\d{4}"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="int3">
		<xs:restriction base="xs:int">
			<xs:pattern value="\d{3}"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="int100">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="100" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="int999">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="999" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="double0_300">
		<xs:restriction base="xs:double">
			<xs:minInclusive value="0.0" />
			<xs:maxInclusive value="100.0" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="string2">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2}"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="string3">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2}"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="string8">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2}"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="string30">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{10}"></xs:pattern>
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="fuel">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Benzin" />
			<xs:enumeration value="Diesel" />
			<xs:enumeration value="Elektro" />
			<xs:enumeration value="Wasserstoff" />
		</xs:restriction>
	</xs:simpleType>


	<xs:complexType name = "wltptyp_km">
		<xs:simpleContent>
			<xs:extension base="double0_300">
				<xs:attribute type="range" default="km" name="unit" use="optional" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:simpleType name="range">
		<xs:restriction base="xs:string">
			<xs:enumeration value="km"/>
			<xs:enumeration value="miles"/>
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name = "consumption">
		<xs:simpleContent>
			<xs:extension base="double0_300">
				<xs:attribute type="l100" default="Liter/100 km" name="unit" use="optional" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:simpleType name="l100">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Liter/100 km"/>
			<xs:enumeration value="gals/100 miles" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name = "co2">
		<xs:simpleContent>
			<xs:extension base="double0_300">
				<xs:attribute type="gkm" default="g/km" name="unit" use="optional" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:simpleType name="gkm">
		<xs:restriction base="xs:string">
			<xs:enumeration value="g/km"/>
			<xs:enumeration value="p/mile"/>
		</xs:restriction>
	</xs:simpleType>



</xs:schema>